    "\n",
    "ar1 = np.random.randint(500,100000,size=5) # 1인당 GDP\n",
    "ar2 = np.random.choice(2, size=5) # 성별\n",
    "ar3 = np.random.randint(5,30, size=5) # 나이\n",
    "ar4 = np.random.randint(0,10, size=5) # 교육 수준\n",
    "ar5 = np.random.randint(1,10, size=5) # 건강 수준\n",
